    "node-sass": "^4.7.2",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1"
  },
  "dependencies": {
    "d3-format": "^1.2.2",
    "doctrine": "^2.1.0",
    "history": "^3.3.0",
    "jss": "^8.1.0",
    "jss-preset-default": "^3.0.0",
    "marked": "^0.3.12",
    "material-ui": "^1.0.0-beta.27",
    "material-ui-icons": "^1.0.0-beta.17",
    "prismjs": "^1.9.0",
    "react-docgen": "^2.20.0",
    "react-redux": "^5.0.6",
