{
  "name": "react-compound-slider-docs",
  "private": true,
  "version": "0.3.0",
  "description": "Next level react slider component",
  "repository": {
    "type": "git",
    "url": "https://github.com/sghall/react-compound-slider.git"
  },
  "scripts": {
    "start": "npm run clean && npm run start:dll && ../node_modules/.bin/babel-node webpack.dev.server.js",
