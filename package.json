{
  "name": "react-electric-slide",
  "version": "0.1.0",
  "description": "An eco friendly alternative to gas powered slider components",
  "keywords": [
    "react",
    "react-component",
    "range-slider",
    "input-range",
    "range",
    "slider",
    "form",
    "input"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sghall/react-electric-slide.git"
  },
  "main": "lib/js/index.js",
  "author": "Steven Hall",
  "license": "MIT",
  "devDependencies": {
    "app-module-path": "^2.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "bundle-inspector-webpack-plugin": "^0.1.9",
    "chai": "^4.0.2",
    "coveralls": "^2.11.15",
    "cross-env": "^5.0.1",
    "enzyme": "^2.8.1",
    "eslint": "^4.2.0",
    "eslint-plugin-prettier": "^2.2.0",
    "flow-bin": "^0.47.0",
    "fs-extra": "^2.1.2",
    "glob": "^7.1.1",
    "husky": "^0.14.3",
    "jsdom": "^11.0.0",
    "json-loader": "^0.5.7",
    "karma": "^1.6.0",
    "karma-browserstack-launcher": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "lint-staged": "^4.0.2",
    "minimist": "^1.2.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nyc": "11.0.1",
    "phantomjs-prebuilt": "^2.1.15",
    "prettier": "^1.5.3",
    "react": "^15.5.3",
    "react-addons-test-utils": "^15.4.0",
    "react-dom": "^15.5.3",
    "recursive-readdir-sync": "^1.0.6",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "webpack": "^3.5.5"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "scripts": {
    "build": "npm run build:es2015 && npm run build:es2015modules && npm run build:copy-files && npm run build:umd:dev && npm run build:umd:min",
    "build:es2015": "cross-env NODE_ENV=production babel ./src --ignore *.spec.js --out-dir ./build",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./build/index.es.js",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "build:umd:dev": "webpack",
    "build:umd:min": "cross-env NODE_ENV=production webpack",
    "clean:build": "rimraf build",
    "lint": "eslint src docs/src test --quiet --cache && echo \"eslint: no lint errors\"",
    "prebuild": "npm run clean:build",
    "test": "cross-env NODE_ENV=test BABEL_ENV=test mocha \"src/**/*.spec.js\"",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "test:watch": "npm run test -- -w",
    "test:coverage": "cross-env NODE_ENV=test BABEL_ENV=coverage nyc mocha \"src/**/*.spec.js\" && nyc report --reporter=lcov",
    "test:coverage:html": "cross-env NODE_ENV=test BABEL_ENV=coverage nyc mocha \"src/**/*.spec.js\" && nyc report --reporter=html",
    "pretty": "prettier --write src/*.js",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "d3-scale": "^1.0.6",
    "deepmerge": "^1.5.1",
    "hoist-non-react-statics": "^2.3.0",
    "prop-types": "^15.5.8",
    "rc-tooltip": "^3.4.8",
    "react-jss": "^7.1.0",
    "recompose": "^0.25.0",
    "resonance": "^0.9.5",
    "shallowequal": "^1.0.2",
    "warning": "^3.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
